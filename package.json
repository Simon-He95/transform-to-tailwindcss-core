{
  "name": "transform-to-tailwindcss-core",
  "type": "module",
  "version": "0.0.32",
  "packageManager": "pnpm@10.10.0",
  "description": "A core library to transform CSS or style objects to Tailwind CSS utility classes. Simplifies migration and integration with Tailwind CSS.",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/transform-to-tailwindcss-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/transform-to-tailwindcss-core.git"
  },
  "bugs": "https://github.com/Simon-He95/transform-to-tailwindcss-core/issues",
  "keywords": [
    "tailwindcss",
    "css",
    "transform",
    "utility",
    "converter",
    "style",
    "migration",
    "core",
    "library"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "run": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown ./src/index.ts",
    "dev": "npm run build -- --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@types/node": "^18.19.98",
    "bumpp": "^8.2.1",
    "eslint": "^9.26.0",
    "esno": "^0.16.3",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "tsdown": "^0.10.2",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vitest": "^3.1.3"
  }
}
